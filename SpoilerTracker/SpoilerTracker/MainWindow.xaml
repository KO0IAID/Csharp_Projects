                        <RowDefinition Height="*"/>
                        <RowDefinition Height="*"/>
                        <RowDefinition Height="*"/>
                        <RowDefinition Height="*"/>
                        <RowDefinition Height="*"/>
                    </Grid.RowDefinitions>

                    <StackPanel Grid.Row="1" Grid.RowSpan="5" Grid.Column="0" Grid.ColumnSpan="2" >
                        <Button x:Name="SpoilerLogBtn" Content="Import Spoiler Log" Click="SpoilerBtn_Click" Margin="0,0,0,0"/>
                        <Button x:Name="ExportToEmotrackerBtn" Content="Export to Emotracker" Grid.ColumnSpan="2" Height="20" FontSize="10" IsEnabled="True" Margin="0,3,0,0" Click="ExportToEmotracker"/>
                    </StackPanel>
                    
                    <StackPanel Grid.Row="1" Grid.RowSpan="5" Grid.Column="2">
                        <TextBlock x:Name="SpoilerLogPrompt" Foreground="White" Margin="5,0,0,0" FontSize="15"/>
                        <TextBlock x:Name="TrackerPrompt" Grid.Row="2" Foreground="White" Margin="5,0,0,0" FontSize="15"/>
                        <TextBlock x:Name="ExportGameSettingsPrompt" Foreground="White" Margin="5,0,0,0" FontSize="15"/>
                    </StackPanel>

                    
                    

                    <StackPanel Grid.Row="14" Grid.RowSpan="2"  Grid.Column="0" Grid.ColumnSpan="8">
                        <CheckBox x:Name="AutoLoadSpoilerCheckBox"
                                  Content="Auto Load Spoilers"
                                  Grid.Row="14"
                                  Grid.Column="0"
                                  Grid.ColumnSpan="3"
                                  VerticalAlignment="Center"
                                  Foreground="White"
                                  Click="AutoLoadSpoilerCheckBox_Click">
                            <CheckBox.ToolTip>
                                <ToolTip>
                                    <TextBlock>
                                            Checked: It will save a folder location.
                                                    <LineBreak />
                                            Then it will automatically load the most recently created spoiler file.
                                                    <LineBreak />
                                            Loads file right away, and will persist on future launches.
                                    </TextBlock>
                                </ToolTip>
                            </CheckBox.ToolTip>
                        </CheckBox>
                        <TextBlock x:Name="FolderPathPrompt" Foreground="LightGray" FontSize="10"/>
                    </StackPanel>

                    <StackPanel Grid.Row="12" Grid.RowSpan="2" Grid.Column="0" Grid.ColumnSpan="8">
                        <TextBlock x:Name="CurrentHeader"  Text="Current:" Foreground="LightGray" FontSize="10"/>
                        <TextBlock x:Name="FileNamePrompt" VerticalAlignment="Bottom" Foreground="LightBlue" FontSize="10" />
                        <TextBlock x:Name="FileDatePrompt" VerticalAlignment="Top" Foreground="LightBlue" FontSize="10"/>
